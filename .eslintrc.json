{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-unused-expressions": 0,
        "no-console": 0,
        "no-undef": 0,
        "class-methods-use-this": 0,
        "no-unused-vars": 0,
        "object-curly-newline": 0
    }
}
